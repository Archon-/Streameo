<html class="no-js" lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <link rel="icon" type="image/png" href="@Url.Content("~/apple-touch-icon.png")" />
    <!--[if IE]>
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")" type="image/vnd.microsoft.icon" />
    <![endif]-->
    <link href="@Url.Content("~/Content/css/style.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.6.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/navigation.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    
    <link href="@Url.Content("~/Content/rateit.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.rateit.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/UploadFileExtensionCheck.js")" type="text/javascript"></script>   
    <script src="../../Scripts/rating.js" type="text/javascript"></script>
    <script src="../../Scripts/Disqus.js" type="text/javascript"></script>

    <script src="../../Scripts/jquery.unobtrusive-ajax.js" type="text/javascript"></script>
    <script src="../../Scripts/MicrosoftAjax.js" type="text/javascript"></script>
    <script src="../../Scripts/MicrosoftMvcAjax.js" type="text/javascript"></script>
</head>
<body>
    <!-- Prompt IE 6 users to install Chrome Frame. Remove this if you support IE 6.
       chromium.org/developers/how-tos/chrome-frame-getting-started -->
    <!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->
    <!-- Add your site or application content here -->
    <header>
        <div id="streameo">
            <img src="@Url.Content("~/Content/images/streameo.jpg")" border="0" alt="Streameo" />
        </div>
        <ul>
            @*<li>@Html.ActionLink("Aktualności", "Index", "Home", null, new { @class = "ajax" })</li>*@
            <li>@Ajax.ActionLink("Aktualności", "Index", "Home", new AjaxOptions()
           {
               InsertionMode = InsertionMode.Replace,
               UpdateTargetId = "main"
           })</li>
            <li>@Ajax.ActionLink("Muzyka", "Index", "Browse", new AjaxOptions()
           {
               InsertionMode = InsertionMode.Replace,
               UpdateTargetId = "main"
           })</li>
            @*<li>@Html.ActionLink("Muzyka", "Index", "Browse", null, new { @class = "ajax" })</li>*@
            @*<li><a href="#" title="Muzyka">Muzyka</a></li>
  		<li><a href="#" title="Radio">Radio</a></li>*@
            <li><a href="#" title="Premium">Premium</a></li>
        </ul>
        @Html.Partial("_LogOnPartial")
        <div class="clr">
        </div>
    </header>
    <section id="control" style="width: 100%; height: 115px">
        @Html.Partial("_playerPartial")
    </section>
    <section id="search">
        <div id="cd1" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd1.jpg")" border="0" alt="" />
        </div>
        <div id="cd2" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd2.jpg")" border="0" alt="" />
        </div>
        <div id="cd3" class="cd">
            <img src="@Url.Content("~/Content/images/cds/vinyl.png")" border="0" alt="" />
        </div>
        <div id="cd4" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd4.jpg")" border="0" alt="" />
        </div>
        <div id="cd5" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd5.jpg")" border="0" alt="" />
        </div>
        <div id="cd6" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd6.jpg")" border="0" alt="" />
        </div>
        <div id="cd7" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd7.jpg")" border="0" alt="" />
        </div>
        <div id="cd8" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd8.jpg")" border="0" alt="" />
        </div>
        <div id="cd9" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cd9.jpg")" border="0" alt="" />
        </div>
        <div id="cd10" class="cd">
            <img src="@Url.Content("~/Content/images/cds/cassette.png")" border="0" alt="" />
        </div>
        <div id="search-container">
            @{ Html.RenderAction("Index", "Search"); }
        </div>
    </section>
    <section id="main">
        @RenderBody()
    </section>
    <footer>
        <div id="footer-inner">
            <p>
                Copyright &copy;2012 Streameo</p>
        </div>
    </footer>
</body>
</html>
